 	<servlet-name>logical2</servlet-name>
 	<url-pattern>/updation</url-pattern>
 </servlet-mapping>
 
 <servlet>
 	<servlet-name>logical3</servlet-name>
 	<servlet-class>com.nt.Select</servlet-class>
 </servlet>
 <servlet-mapping>
 	<servlet-name>logical3</servlet-name>
 	<url-pattern>/select</url-pattern>
 </servlet-mapping>
 
 <servlet>
 	<servlet-name>logical4</servlet-name>
 	<servlet-class>com.nt.SelectAll</servlet-class>
 </servlet>
